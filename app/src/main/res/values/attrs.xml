<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SeekBar">
        <attr name="android:text" format="string" />
        <attr name="android:textColor" format="color" />
        <attr name="android:textSize" format="dimension" />
        <attr name="android:max" format="integer" />
        <attr name="android:progress" format="integer" />
    </declare-styleable>


    <declare-styleable name = "ColorPane">
        <attr name="android:color" format="color" />
        <attr name="android:strokeColor" format="color" />
        <attr name="android:strokeWidth" format="dimension" />
        <attr name="android:checked" format="boolean" />
    </declare-styleable>
</resources>